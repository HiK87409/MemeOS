{
  "name": "memeos",
  "version": "1.0.5",
  "description": "MemeOS笔记应用",
  "scripts": {
    "install:all": "cd client && npm install && cd ../server && npm install",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "start:all": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "cd client && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "notes",
    "memos",
    "react",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "js-sha256": "^0.11.1",
    "node-fetch": "^2.7.0"
  }
}
